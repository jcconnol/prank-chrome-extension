{
    "manifest_version": 2,
    "name": "Prankware",
    "description": "This extension allows for the setting of pranks.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
     "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "web_accessible_resources": [
        "cageImgs/*.jpg"
    ]
  }